{
  "name": "node-package-starter-kit",
  "version": "0.1.0",
  "description": "A starter kit for node packages",
  "main": "lib/index.js",
  "jsnext:main": "src/index.js",
  "files": [
    "lib",
    "src"
  ],
  "scripts": {
    "clean": "rimraf lib",
    "build": "babel src -d lib",
    "watch": "babel src -w -d lib",
    "prepublish": "npm run clean && npm run build",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/gkcgautam/node-package-starter-kit.git"
  },
  "keywords": [
    "node",
    "package",
    "npm",
    "starter"
  ],
  "author": "Gautam Chaudhary <gautam@gautamchaudhary.com> (http://www.gautamchaudhary.com)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/gkcgautam/node-package-starter-kit/issues"
  },
  "homepage": "https://github.com/gkcgautam/node-package-starter-kit#readme",
  "dependencies": {
    "socket.io": "^1.4.8",
    "socketio-wildcard": "^0.3.0"
  },
  "devDependencies": {
    "babel-cli": "^6.14.0",
    "babel-preset-es2015": "^6.14.0",
    "rimraf": "^2.5.4"
  }
}
